            Strasse:
              type: string
            Hausnummer:
              type: string
            Plz:
              type: string
            Ort:
              type: string
            Land:
              type: string
        EmailAdresse:
          type: string
          format: email
        Telefonnummer:
          type: string
        GeburtsGruendungsdatum:
          type: string
          format: date-time
        SteuerIdentifikationsnummer:
          type: string
        ZustaendigesFinanzamt:
          type: string
